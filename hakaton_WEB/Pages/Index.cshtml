@page
@model IndexModel
@{
    ViewData["Title"] = "Авторизация";
}

<h1>Авторизация пользователя</h1>
<h2>Тесты</h2>
<ul>
    @foreach (var test in Model.test)
    {
        <form method="post">
            <li>@test.ArticleTest: @test.CompetencyId</li>
            <button type="submit">Авторизоваться</button>
        </form>
    }
</ul>
